{
  "name": "geekhack-scraper",
  "version": "1.0.0",
  "description": "scraps geekhack and displays content all together making it easier to keep up with keyboard related products",
  "main": "index.js",
  "scripts": {
    "scrape": "cd geekhack-scraper && node index.js",
    "start-debug": "cd website && vue-cli-service serve",
    "start-build": "cd website && vue-cli-service build",
    "vue-lint": "cd website && vue-cli-service lint",
    "start-server": "cd server && node src/app.js",
    "server-lint": "cd server && eslint **/*.js"
  },
  "keywords": [
    "geek",
    "hack",
    "geekhack",
    "keyboard",
    "groupbuy",
    "group",
    "buy",
    "interest",
    "check"
  ],
  "author": "Gobluebro",
  "repository": {
    "type": "git",
    "url": "git://github.com/Gobluebro/Geekhack-scraper.git"
  },
  "license": "MIT",
  "dependencies": {
    "@jinphen/download2": "^7.1.2",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "download": "^7.1.0",
    "express": "^4.17.1",
    "jsdom": "^15.2.1",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "pg": "^7.18.2",
    "sequelize": "^5.22.3"
  },
  "devDependencies": {
    "tailwindcss": "^1.9.6"
  }
}
